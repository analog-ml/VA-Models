`MPRco(THESATG        ,0.0        ,"V^-1"     ,-0.5        ,inf         ,"Gate voltage dependence of velocity saturation")
`MPRco(THESATT        ,1.0        ,""         ,0.01        ,inf         ,"Threshold parameter of gate voltage dependence of velocity saturation")

//  Linear to saturation transition parameters
`MPRco(AX             ,8.0        ,""         ,2.0         ,inf         ,"Linear/saturation transition factor")

//  Channel length modulation parameters
`MPRcz(ALP            ,1.0e-02    ,""                                   ,"CLM pre-factor")
